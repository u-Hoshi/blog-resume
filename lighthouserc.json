{
  "ci": {
    "collect": {
      "numberOfRuns": 3,
      "startServerCommand": "yarn start",
      "url": [
        "http://localhost:3000/",
        "http://localhost:3000/resume",
        "http://localhost:3000/blog"
      ],
      "settings": {
        "maxWaitForLoad": 90000
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
