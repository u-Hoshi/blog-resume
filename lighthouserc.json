{
  "ci": {
    "collect": {
      "numberOfRuns": 2,
      "startServerCommand": "yarn start",
      "url": ["http://localhost:3000/", "http://localhost:3000/resume"],
      "settings": {
        "maxWaitForLoad": 90000
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
